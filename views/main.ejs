        <!-- Portfolio section -->
        <section class="portfolio section" id="portfolio">
            <h2 class="section__title">Portfolio</h2>
            <span class="section__subtitle">My favorite projects</span>

            <div class="portfolio__container container swiper-container">
                <div class="swiper-wrapper">
                    <!-- project 1 -->
                    <div class="portfolio__content grid swiper-slide">
                        <img src="../public/img/proj1.jpg" alt="project 1" class="portfolio__img">

                        <div class="portfolio__data">
                            <h3 class="portfolio__title">Custom website</h3>
                            <p class="portfolio__description">Website (template) adaptable to all devices, using Materialize</p>
                            <a href="https://nataliaermurachi.github.io/luxuryrestoreservices/" target="_blank" class="button button--flex button--small portfolio__button">Go to the project
                              <i class="fa fa-arrow-right  button__icon"></i>
                            </a>
                        </div>
                    </div>
                    <!-- project 2 -->
                    <div class="portfolio__content grid swiper-slide">
                        <img src="../public/img/proj2.jpg" alt="project 1" class="portfolio__img">

                        <div class="portfolio__data">
                            <h3 class="portfolio__title">React burger-builder app</h3>
                            <p class="portfolio__description">A funny, practice implementation with redux and react component's ui</p>
