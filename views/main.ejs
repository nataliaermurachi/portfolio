            <h2 class="section__title">My Skills</h2>

            <div class="skills__container container grid">
                <div>
                    <!-- skill 1 -->
                    <div class="skills__content">
                        <div class="skills__header">
                            <i class="fa fa-code skills__icon"></i>

                            <div>
                                <h1 class="skills__titles">Frontend developer</h1>
                                <span class="skills__subtitle">Familiar with HTML, CSS, JavaScript, SASS, Bootstrap, Materialize, BEM, React</span>
