                    <i class="fa fa-angle-right swiper-portfolio-icon"></i>
                </div>
                <div class="swiper-button-prev">
                    <i class="fa fa-angle-left swiper-portfolio-icon"></i>
                </div>
                <!-- pagination -->
                <div class="swiper-pagination"></div>
            </div>
        </section>
        <!-- Contact section -->
        <section class="contact section" id="contact">
            <h2 class="section__title">Contact Me</h2>
            <span class="section__subtitle">Get in touch</span>

            <div class="contact__container container grid">
                <div>
                    <div class="contact__information">
                        <i class="fa fa-phone contact__icon"></i>
                        <div>
                            <h3 class="contact__title">Call Me</h3>
                            <span class="contact__subtitle">+31-61-588-3034</span>
                        </div>
                    </div>
                    <div class="contact__information">
                        <i class="fa fa-envelope contact__icon"></i>
                        <div>
                            <h3 class="contact__title">Email</h3>
                            <span class="contact__subtitle">natalia.rotaru.f@gmail.com</span>
                        </div>
                    </div>
                    <div class="contact__information">
                        <i class="fa fa-map-marker contact__icon"></i>
                        <div>
                            <h3 class="contact__title">Location</h3>
                            <span class="contact__subtitle">Hoofddorp, 2135GT, The Netherlands</span>
                        </div>
                    </div>
                </div>
                <form method="POST" action="/sendemail" class="contact__form grid">
                    <div class="contact__inputs grid">
                        <div class="contact__content">
                            <label for="fName" class="contact__label">Name</label>
                            <input type="text" name="fName" id="fName" class="contact__input" required>
                        </div>
                        <div class="contact__content">
                            <label for="email" class="contact__label">Email</label>
                            <input type="email" id="email" name="email" class="contact__input" required>
                        </div>
                    </div>
                    <div class="contact__content">
                        <label for="message" class="contact__label">Message</label>
                        <textarea name="message" id="message" cols="0" rows="7" class="contact__input" required></textarea>
                    </div>
                    <div>
                        <button type="submit" class="button button--flex submit" id="send-message"> Send Message
                        </button>
                    </div>
                </form>
            </div>
