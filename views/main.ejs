                            <div></div>

                            <div>
                                <span class="qualification__rounder"></span>
                                <!-- <span class="qualification__line"></span> -->
                            </div>
                            <div>
                                <h3 class="qualification__title">Intern</h3>
                                <span class="qualification__subtitle">Miso Textile MD</span>
                                <div class="qualification__calendar">
                                    <i class="fa fa-calendar"></i> Dec 2017 - Aug 2018
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
